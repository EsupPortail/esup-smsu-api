<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd "
	>

	<bean
		id="domainService"
		class="org.esupportail.smsuapi.domain.DomainService"
		>
		<property name="daoService" ref="daoService" />
		<property name="sendSmsManager" ref="sendSmsManager"/>
		<property name="blackListManager" ref="blackListManager"/>
		<property name="reportingManager" ref="reportingManager"/>
		<property name="clientManager" ref="clientManager"/>
	</bean>

</beans>
